Algoritmo "NumeroPrimo"

Var
   cont, N, ContDiv:inteiro
Inicio
      cont <- 1
      ContDiv <- 0
      escreva("Digite um número: ")
      leia(N)
      Repita
          Se (N % cont = 0) entao
          ContDiv <- ContDiv + 1
          FimSE
          cont <- cont + 1
      Ate(cont > N)
      Se (ContDiv > 2) entao
      escreval(" O valor", N, " não é um número primo.")
      senao
      escreval(" O valor", N, " é um número primo.")
      FimSe
Fimalgoritmo
