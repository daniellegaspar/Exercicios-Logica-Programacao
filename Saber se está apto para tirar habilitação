Algoritmo "Habilitação"

Var
   ano, nasc, idade:inteiro

Inicio
           escreva("Em que ano estamos? (yyyy) ")
           leia(ano)
           Escreva("Em que ano você nasceu? (yyyy) ")
           leia(nasc)
           idade <- ano - nasc
           escrevaL("Sua idade é: ", idade, " anos")
           SE (idade >= 18) entao
              escreva("Parabéns você já pode tirar sua habilitação.")
              senao
              escreva("Você não está na idade adequada para tirar habilitação.")
              fimse

Fimalgoritmo
