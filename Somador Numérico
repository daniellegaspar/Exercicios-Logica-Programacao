Algoritmo "SomadorNum√©rico"

Var

   cont, N, S:inteiro

Inicio

     cont <- 1
     S <- 0
     enquanto (cont <= 5) faca
        EscrevaL("Digite o", cont, "o. valor : ")
        leia(N)
        S <- S + N
        cont <- cont + 1
     FimEnquanto
     EscrevaL("A soma de todos os valores foi: ", S)



Fimalgoritmo
